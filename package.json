{
  "name": "nodejs-unit-test",
  "version": "1.0.0",
  "description": "Jest Unit Test",
  "main": "./src/index.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "test-dev": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "maxConcurrency": 5,
    "verbose": true,
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "function": 80,
        "lines": 80,
        "statement": 80
      }
    }
  },
  "author": "Andrian17",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@types/jest": "^29.5.11",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0"
  },
  "dependencies": {
    "winston": "^3.11.0"
  }
}
